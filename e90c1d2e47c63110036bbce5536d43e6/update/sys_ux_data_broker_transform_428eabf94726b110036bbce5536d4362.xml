        <script><![CDATA[function transform(input) {

//gs.info('input : ' + JSON.stringify(input));
	orderLineItemSysId = input.recordSysId;

	gs.info('orderLineItemSysId :' + orderLineItemSysId);

	var productOrderGr = new GlideRecord('sn_ind_tmt_orm_product_order');
	productOrderGr.addQuery('order_line_item', orderLineItemSysId);
	productOrderGr.query();

	if(productOrderGr.next()){
		//gs.info('productOrderGr.getUniqueValue(); ' + productOrderGr.getUniqueValue());
		var output = productOrderGr.getUniqueValue();
	}
	gs.info('output : ' + output);
	gs.info('output type: ' + typeof output);
	return output;
}]]></script>
